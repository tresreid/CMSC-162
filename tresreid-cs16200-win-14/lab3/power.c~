//power.c

#include <stdio.h>
#include <string.h>



  int main( int argc, char *argv[]){
    int x;
    int n;
    sscanf( argv[1],"%d", &x);
    sscanf( argv[2],"%d", &n);
    printf("%d\n", power(x,n));
    printf("%d\n", x);
    printf("%d\n", n);
    
}
  int power(int y,int m){
    if(m == 0){
      return 1;
    }
    else if (m==1){
      return y;
    }
    else if ((m % 2) == 0){
      return power(y*y, m/2);
    }
    else if ((m % 2) == 1){
      return y * power(y*y, (m-1)/2);
    }
  }
