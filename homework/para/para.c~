// Top level code for atomic-weight calculator

#include <stdio.h>
#include <stdlib.h>
#include <sys/types.h>
#include <sys/stat.h>
#include <sys/param.h>

char *progname;

int yylex(void);
int yyparse(void);
void parse_string(char *str);

int main(int argc, char **argv) {
	progname = argv[0];
	
	if (argc > 1) {
		for (int i = 1; i < argc; ++i) {
			parse_string(argv[i]);
		}
	} else {
		yyparse();
	}
	exit(0);
}

